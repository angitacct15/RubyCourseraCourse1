<h1>Searching for - <%= @search_term %></h1>
<table border="1">
	<tr>
		<!--<th>Image</th> -->
		<th>Name</th>
		<th>Description</th>
	</tr>
	<!-- <% %> are called evaluators -->
	<% @courses.each do |course| %> 
	
		<tr class=<%= cycle('even', 'odd') %>>
			<!--<td><%= image_tag(course["photoUrl"]) %></td> -->
			<td><%= course["name"] %></td>
			<td><%= course["slug"] %></td>
		</tr>
	
	<% end %>
</table>
